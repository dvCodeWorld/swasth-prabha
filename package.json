{
  "name": "swasth-prabha",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "backend": "json-server --watch db.json --port 3000",
    "dev": "concurrently \"npm run backend\" \"npm start\""
